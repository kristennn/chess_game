<%= render "form"%>
<h5>下线玩家</h5>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>玩家ID</th>
      <th>玩家昵称</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.account %></td>
      </tr>
    <% end %>
  </tbody>
</table>
