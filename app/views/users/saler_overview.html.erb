      <i class="fa fa-circle-thin fa-stack-2x"></i>
      <i class="fa fa-file-text fa-stack-1x"></i>
    </span><br>
    <div class="text-info">
      提现记录
    </div>
    <% end %>
  </div>

  <div class="col-xs-3 col-md-3">
    <%= link_to edit_user_path(current_user) do %>
    <span class="fa-stack fa-3x">
      <i class="fa fa-circle-thin fa-stack-2x"></i>
      <i class="fa fa-key fa-stack-1x"></i>
    </span><br>
    <div class="text-info">
