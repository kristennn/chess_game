<%= render "form" %>
<div class="container-row">
  <%= link_to saler_overview_path do %>
    <h5>菜单选项</h5>
  <% end %>
</div>
  <div class="col-xs-3 col-md-3">
    <%= link_to  saler_info_path do %>
