<div class="col-md-12">
    <h5>所有推广员 <%= link_to "新增推广员", signup_path, class:"btn btn-default-1" %></h5>

  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th width="20%">代理名称</th>
        <th>代理ID</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
         <% unless user.permission == "manager" %>
           <td><%= user.name %></td>
           <td><%= user.id %></td>
         <% end %>
        </tr>
      <% end %>
   </tbpdy>
  </table>
  <div class="text-right">
    <%= paginate @users, window:2 %>
  </div>
</div>
